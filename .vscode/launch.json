        {
            "name": "Python Debugger: Module",
            "type": "debugpy",
            "request": "launch",
            "module": "tests.test_impulse_run"
        },
        {
            "name": "test_run_reader",
            "type": "debugpy",
            "request": "launch",
            "module": "tests.test_run_reader_machine"
        },
        {
            "name": "test_autocheck",
            "type": "debugpy",
            "request": "launch",
            "module": "tests.test_autocheck"
        }
    ]
